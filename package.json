{
  "name": "fossbilling",
  "author": "FOSSBilling Team",
  "homepage": "https://fossbilling.org",
  "license": "Apache License 2.0",
  "scripts": {
    "build": "npm run build-themes && npm run build-modules",
    "build-themes": "npm run build-admin_default && npm run build -w huraga",
    "build-modules": "npm run build-wysiwyg",
    "build-huraga": "npm run build -w huraga",
    "build-admin_default": "npm run build -w admin_default",
    "build-wysiwyg": "npm run build -w wysiwyg",
    "cypress:open": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FOSSBilling/FOSSBilling.git"
  },
  "issues": {
    "url": "https://github.com/FOSSBilling/FOSSBilling/issues"
  },
  "dependencies": {
    "autosize": "^6.0.1",
    "bootstrap": "^5.3.3",
    "flag-icons": "^7.2.3",
    "tom-select": "^2.3.1"
  },
  "devDependencies": {
    "@symfony/webpack-encore": "5.2.0",
    "autoprefixer": "10.4.21",
    "core-js": "3.46.0",
    "cypress": "15.5.0",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.0",
    "sass": "1.93.3",
    "sass-embedded": "1.93.3",
    "sass-loader": "16.0.6",
    "svg-sprite-loader": "6.0.11",
    "svgo-loader": "4.0.0",
    "webpack": "5.102.1",
    "webpack-cli": "6.0.1"
  },
  "workspaces": [
    "src/themes/huraga",
    "src/themes/admin_default",
    "src/modules/Wysiwyg"
  ],
  "config": {
    "platform": {
      "php": "8.2"
    }
  },
  "engines": {
    "npm": ">=10"
  },
  "overrides": {
    "@melloware/coloris": "^0.25.0",
    "@tabler/core": {
      "apexcharts": "^5.0.0"
    },
    "autosize": "^6.0.1",
    "braces": "^3.0.3",
    "debug": "^4.0.0",
    "flat": "^6.0.0",
    "imask": "^7.6.1",
    "micromatch": "^4.0.8",
    "minimatch": "^10.0.0",
    "postcss": "$postcss"
  }
}
