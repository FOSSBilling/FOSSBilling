  "repository": {
    "type": "git",
    "url": "https://github.com/FOSSBilling/FOSSBilling.git"
  },
  "issues": {
    "url": "https://github.com/FOSSBilling/FOSSBilling/issues"
  },
  "devDependencies": {
    "@symfony/webpack-encore": "^4.3.0",
    "autoprefixer": "^10.4.14",
    "core-js": "^3.31.0",
    "cypress": "^12.14.0",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.3",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^4.0.0",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "src/themes/admin_default",
    "src/modules/Wysiwyg"
  ],
  "config": {
    "platform": {
      "php": "8.0"
    }
  },
  "engines": {
    "npm": ">=9"
  }
